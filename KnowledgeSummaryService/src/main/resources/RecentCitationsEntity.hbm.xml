<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.utah.edu.semmedDao">

    <class name="org.utah.edu.semmed.db.domain.RecentCitationsEntity" table="RECENT_CITATIONS">
        <id name="pmid" column="PMID"/>
        <property name="issn" column="ISSN"/>
        <property name="da" column="DA"/>
        <property name="dcom" column="DCOM"/>
        <property name="dp" column="DP"/>
        <property name="pYear" column="PYear"/>
        <property name="quality" column="Quality"/>
        <property name="probability" column="Probability"/>
        <property name="journalId" column="JournalID"/>
        <property name="journalAbbreviation" column="journal_abbreviation"/>
        <property name="numComments" column="num_comments"/>
        <property name="ctGovId" column="CT_GOV_ID"/>
        <property name="pmcId" column="PMC_ID"/>
        <property name="doi" column="DOI"/>
        <property name="comparativeStudy" column="comparative_study"/>
        <property name="authors" column="authors"/>
        <property name="fundingSource" column="funding_source"/>
        <property name="actualSample" column="actual_sample"/>
        <property name="ctGovFundingSource" column="ct_gov_funding_source"/>
        <property name="ctGovFundingSourceType" column="ct_gov_funding_source_type"/>
        <property name="processed" column="processed"/>
        <property name="summarySentence" column="summary_sentence"/>
        <property name="citationjson" column="Citationjson"/>
    </class>
</hibernate-mapping>